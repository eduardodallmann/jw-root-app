<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>JW Project</title>
  <script src="https://cdn.jsdelivr.net/npm/regenerator-runtime@0.13.7/runtime.min.js"></script>
  <meta http-equiv="Content-Security-Policy" content="default-src 'self' https: localhost:*; script-src 'unsafe-inline' 'unsafe-eval' https: localhost:*; connect-src https: localhost:* ws://localhost:*; style-src 'unsafe-inline' https:; object-src 'none';">
  <meta name="importmap-type" content="systemjs-importmap" />
  <script type="systemjs-importmap">
    {
      "imports": {
        "single-spa": "https://cdn.jsdelivr.net/npm/single-spa@latest/lib/system/single-spa.min.js",
        "react": "https://cdn.jsdelivr.net/npm/react@latest/umd/react.production.min.js",
        "react-dom": "https://cdn.jsdelivr.net/npm/react-dom@latest/umd/react-dom.production.min.js",
        "@mui/material": "https://unpkg.com/@mui/material@latest/umd/material-ui.production.min.js",
        "@emotion/react": "https://cdn.jsdelivr.net/npm/@emotion/react@latest/dist/emotion-react.umd.min.js",
        "@emotion/styled": "https://cdn.jsdelivr.net/npm/@emotion/styled@latest/dist/emotion-styled.umd.min.js",
        "jotai": "https://cdn.jsdelivr.net/npm/jotai@latest/index.min.js",
        "moment": "https://cdn.jsdelivr.net/npm/moment@latest/moment.min.js"
      }
    }
  </script>
  <link rel="preload" href="https://cdn.jsdelivr.net/npm/single-spa@5.9.0/lib/system/single-spa.min.js" as="script">

  <% if (isLocal) { %>
  <script type="systemjs-importmap">
    {
        "imports": {
          "@jw-project/root-config": "//localhost:9000/jw-root-config.js",
          "@jw-project/home-app": "//localhost:8081/jw-project-home-app.js"
        }
      }
    </script>
  <% } else { %>
  <script type="systemjs-importmap">
    {
        "imports": {
          "@jw-project/root-config": "//jw-project-58cb8.web.app/jw-root-config.js"
        }
      }
    </script>
  <% } %>
  <script type="systemjs-importmap" src="https://jw-project-58cb8-static.web.app/systemjs.json"></script>

  <script src="https://cdn.jsdelivr.net/npm/import-map-overrides@2.2.0/dist/import-map-overrides.js"></script>
  <% if (isLocal) { %>
  <script src="https://cdn.jsdelivr.net/npm/systemjs@6.8.3/dist/system.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/systemjs@6.8.3/dist/extras/amd.js"></script>
  <% } else { %>
  <script src="https://cdn.jsdelivr.net/npm/systemjs@6.8.3/dist/system.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/systemjs@6.8.3/dist/extras/amd.min.js"></script>
  <% } %>
</head>

<body>
  <noscript>
    You need to enable JavaScript to run this app.
  </noscript>
  <div id="root-loading" style="display: none;">
    <!-- <div id="single-spa-application:@jw-project/app-loading"></div> -->
  </div>
  <div id="root-login" style="display: none;">
    <div id="single-spa-application:@jw-project/app-login"></div>
  </div>
  <div id="root-apps" style="display: none;">
    <div id="single-spa-application:@jw-project/app-navbar"></div>
    <div id="side-by-side-menu-apps">
      <div id="single-spa-application:@jw-project/app-menu"></div>
      <div id="root-apps-route"></div>
    </div>
  </div>
  <script>
    System.import('@jw-project/root-config');
  </script>
  <import-map-overrides-full show-when-local-storage="devtools" dev-libs></import-map-overrides-full>
</body>

</html>